{
  "name": "swagchat-sdk",
  "version": "0.10.1",
  "description": "swagchat SDK - TypeScript & JavaScript -",
  "keywords": [
    "swagchat",
    "chat",
    "messaging",
    "sdk",
    "typescript"
  ],
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build:lib": "./node_modules/.bin/tsc --pretty --watch",
    "build:browser": "./node_modules/.bin/watchify src/index.ts -s SwagChat -p [tsify] --verbose -o dist/swagchat-sdk.min.js",
    "build:doc": "./node_modules/.bin/typedoc --out ./doc/ ./src/",
    "publish": "rm -rf doc && rm -rf dist && rm -rf lib && mkdir dist && ./node_modules/.bin/typedoc --out ./doc/ ./src/ && ./node_modules/.bin/tsc && ./node_modules/.bin/browserify src/index.ts -s SwagChat -p [tsify] -o dist/swagchat-sdk.min.js"
  },
  "author": "Betchi <shinichi.minobe@gmail.com>",
  "contributors": [
    {
      "name": "Betchi",
      "email": "shinichi.minobe@gmail.com"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git://github.com/swagchat/swagchat-sdk-js"
  },
  "license": "MIT",
  "dependencies": {
    "history": "^4.7.2",
    "isomorphic-fetch": "^2.2.1",
    "react": "15.5.4",
    "react-dom": "15.5.4",
    "react-redux": "5.0.5",
    "react-router-redux": "5.0.0-alpha.6",
    "react-scroll": "1.5.2",
    "redux": "3.7.1",
    "redux-logger": "3.0.6",
    "redux-saga": "0.15.3",
    "ws": "^2.2.1"
  },
  "devDependencies": {
    "@types/history": "^4.6.0",
    "@types/isomorphic-fetch": "0.0.34",
    "@types/node": "^8.0.8",
    "@types/react": "^16.0.5",
    "@types/react-dom": "15.5.0",
    "@types/react-redux": "4.4.40",
    "@types/react-scroll": "1.5.0",
    "@types/redux-logger": "3.0.0",
    "@types/ws": "0.0.39",
    "ts-loader": "^2.3.4",
    "tsify": "^3.0.1",
    "tslint": "^5.7.0",
    "tslint-loader": "^3.5.3",
    "typedoc": "^0.8.0",
    "typescript": "^2.4.2",
    "watchify": "^3.9.0"
  },
  "optionalDependencies": {
    "bufferutil": "^3.0.0",
    "utf-8-validate": "^3.0.1"
  }
}
